NEXT_PUBLIC_ENABLE_SMS_NOTIFICATIONS=false
NEXT_PUBLIC_MAINTENANCE_MODE=false

# ============================================
# API RATE LIMITS
# ============================================

# Requests per minute per IP
RATE_LIMIT_PER_MINUTE=60

# Max file upload size (in MB)
MAX_FILE_UPLOAD_SIZE=10

# ============================================
# DATABASE INDEXES (Optional)
# ============================================
# If using additional databases

# MongoDB
# MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/kashpages?retryWrites=true&w=majority

# PostgreSQL
# DATABASE_URL=postgresql://user:password@localhost:5432/kashpages

# ============================================
# WEBHOOKS (Optional)
# ============================================

# Webhook secret for verifying incoming webhooks
WEBHOOK_SECRET=your_webhook_secret_key

# ============================================
# LOGGING & DEBUGGING
# ============================================

# Log level (debug, info, warn, error)
LOG_LEVEL=info

# Enable debug mode
NEXT_PUBLIC_DEBUG=false

# ============================================
# NOTES
# ============================================
# 1. Variables prefixed with NEXT_PUBLIC_ are exposed to the browser
# 2. Keep server-side secrets (without NEXT_PUBLIC_) private
# 3. Use strong, random values for all secrets
# 4. Different values for development, staging, and production
# 5. Never commit .env.local to version control
# 6. Add .env.local to .gitignore
